<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="org.devgeeks.Canvas2ImagePlugin"
    version="0.2.0">

    <name>Canvas 2 Image</name>

    <asset src="www/Canvas2ImagePlugin.js" target="Canvas2ImagePlugin.js" />

    <!-- ios -->
    <platform name="ios">
        <config-file target="config.xml" parent="/widget/plugins">
            <plugins-plist key="Canvas2ImagePlugin" string="Canvas2ImagePlugin" />
        </config-file>

        <header-file src="src/ios/Canvas2ImagePlugin.h" />

        <source-file src="src/ios/Canvas2ImagePlugin.m" />
    </platform>

</plugin>
